    "formsy-react-components": "https://github.com/Mokto/formsy-react-components#form-component",
    "mantra-core": "^1.2.0",
    "react": "^15.1.0",
    "react-addons-create-fragment": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-linked-state-mixin": "^15.1.0",
    "react-addons-perf": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-addons-transition-group": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-mounter": "^1.2.0"
