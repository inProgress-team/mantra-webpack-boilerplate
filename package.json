{
  "name": "kickstart-mantrajs-webpack",
  "version": "1.0.0",
  "description": "Kickstart a project with Meteor, React and react-router fast!",
  "main": "src/server.js",
  "browser": "src/client.js",
  "scripts": {
    "start": "meteor",
    "lint": "eslint ./src/lib ./src/client ./src/server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "test": "NODE_PATH=./src mocha .scripts/mocha_boot.js src/client/**/tests/**/*.js --compilers js:babel-core/register",
    "test-watch": "npm test -- --watch --watch-extensions js,jsx",
    "storybook": "start-storybook -p 9001"
  },
  "dependencies": {
    "babel-eslint": "^6.0.4",
    "domready": "^1.0.8",
    "eslint": "^2.12.0",
